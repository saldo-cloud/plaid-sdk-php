{
    "name": "saldo-cloud/plaid-sdk-php",
    "description": "PHP implementation of Plaid's API as an SDK.",
    "keywords": [
        "plaid",
        "sdk",
        "php"
    ],
    "type": "library",
    "require": {
        "php": ">=8.3",
        "ext-json": "*",
        "ext-curl": "*",
        "nimbly/shuttle": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SaldoCloud\\Plaid\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3.5",
        "vimeo/psalm": "^6.9.4",
        "symfony/var-dumper": "^v7.3.2",
        "php-coveralls/php-coveralls": "^v2.5.3",
        "rector/rector": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "SaldoCloud\\Plaid\\Tests\\": "tests"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ricardo Di Zio",
            "email": "ricardodizio@saldo.cloud"
        }
    ]
}
