{
    "name": "saldo-cloud/plaid-sdk-php",
    "description": "PHP implementation of Plaid's API as an SDK.",
    "keywords": [
        "plaid",
        "sdk",
        "php"
    ],
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "nimbly/shuttle": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "SaldoCloud\\Plaid\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.0",
        "symfony/var-dumper": "^4.2",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "SaldoCloud\\Plaid\\Tests\\": "tests"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ricardo Di Zio",
            "email": "ricardodizio@saldo.cloud"
        }
    ]
}
